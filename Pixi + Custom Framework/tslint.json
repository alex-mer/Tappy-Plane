{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],

  "jsRules": {},
  "rules": {
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ],
    "eofline": false,
    "no-trailing-whitespace": false,
    "comment-format": false,
    "quotemark": false,
    "no-console": false,
    "one-line": false,
    "no-consecutive-blank-lines": false,
    "curly": false,
    "no-unused-expression": false
  },

  "rulesDirectory": []
}
